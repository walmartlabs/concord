<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%-5level] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.walmartlabs.concord.plugins.fs" level="WARN"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
