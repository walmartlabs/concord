<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="2150000" author="ybrigo@gmail.com">
        <addColumn tableName="PROCESS_QUEUE">
            <column name="LATEST_ATTEMPT_NUMBER" type="int">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>

    <changeSet id="2150001" author="ybrigo@gmail.com">
        <addColumn tableName="PROCESS_LOG_SEGMENTS">
            <column name="ATTEMPT_NUMBER" type="int">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>
